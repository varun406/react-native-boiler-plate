{
  "name": "BoilerPlate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env ENVFILE=environments/.env.dev  react-native run-android --mode=devDebug",
    "ios": "react-native run-ios",
    "lint": "eslint ./",
    "start": "react-native start",
    "test": "jest",
    "android-clear": "cd android && gradlew clean",
    "dev:android": "cross-env ENVFILE=environments/.env.dev  react-native run-android --mode=devDebug",
    "uat:android": "cross-env ENVFILE=environments/.env.uat  react-native run-android --mode=uatDebug",
    "prod:android": "cross-env ENVFILE=environments/.env.prod  react-native run-android --mode=prodDebug",
    "dev:android-build": "cross-env ENVFILE=environments/.env.dev --mode=devRelease cd android && gradlew assembleRelease",
    "uat:android-build": "cross-env ENVFILE=environments/.env.uat --mode=uatRelease cd android && gradlew assembleRelease",
    "prod:android-build": "cross-env ENVFILE=environments/.env.prod --mode=prodRelease cd android && gradlew assembleRelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.2.1",
    "@react-navigation/drawer": "^7.1.2",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "cross-env": "^7.0.3",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-config": "^1.5.5",
    "react-native-gesture-handler": "^2.24.0",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-screens": "^4.9.1",
    "react-native-toast-message": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.7",
    "@react-native/eslint-config": "0.76.7",
    "@react-native/metro-config": "0.76.7",
    "@react-native/typescript-config": "0.76.7",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
